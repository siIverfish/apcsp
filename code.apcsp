PROCEDURE display_twice (value)
{
    DISPLAY(value)
    DISPLAY(value)
}

PROCEDURE display_eight (value)
{
    PROCEDURE display_four (value)
    {
        display_twice (value)
        display_twice (value)
    }

    display_four (value)
    display_four (value)
}

